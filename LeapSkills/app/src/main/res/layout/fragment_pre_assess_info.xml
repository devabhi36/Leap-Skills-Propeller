<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    xmlns:android="http://schemas.android.com/apk/res/android">
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_scattered_triangles1">
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/r13">
    <TextView
        android:layout_marginTop="60dp"
        android:id="@+id/trainer_feedback1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="PRE ASSESSMENT INFORMATION"
        android:textAppearance="@style/TitleText1"
        android:background="@drawable/title_fill"/>

    <RelativeLayout
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/trainer_feedback1"
        android:padding="20dp">

        <android.support.v7.widget.CardView
            android:id="@+id/card1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#fff"
            android:layout_centerHorizontal="true">
    <HorizontalScrollView android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="15dp">

    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StudentFeedback">

    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="7"
        android:id="@+id/attendence_table5">

        <TableRow
            android:layout_marginBottom="20dp">

            <TextView
                android:layout_width="90dp"
                android:layout_marginRight="5dp"
                android:layout_weight="5"
                android:fontFamily="@font/worksans_medium"
                android:textStyle="bold"
                android:text="Leap ID"
                android:textColor="@color/button"
                android:textSize="16dp" />

            <TextView
                android:layout_width="100dp"
                android:layout_marginRight="5dp"
                android:layout_weight="5"
                android:fontFamily="@font/worksans_medium"
                android:textStyle="bold"
                android:text="First Name"
                android:textColor="@color/button"
                android:textSize="16dp" />

            <TextView
                android:layout_width="100dp"
                android:layout_marginRight="5dp"
                android:layout_weight="5"
                android:fontFamily="@font/worksans_medium"
                android:textStyle="bold"
                android:text="Last Name"
                android:textColor="@color/button"
                android:textSize="16dp" />
            <TextView
                android:layout_weight="5"
                android:layout_width="130dp"
                android:layout_marginRight="5dp"
                android:text="College Name"
                android:fontFamily="@font/worksans_medium"
                android:textStyle="bold"
                android:textColor="@color/button"
                android:textSize="16dp"/>
            <TextView
                android:layout_weight="5"
                android:layout_width="90dp"
                android:layout_marginRight="5dp"
                android:text="Stream"
                android:fontFamily="@font/worksans_medium"
                android:textStyle="bold"
                android:textColor="@color/button"
                android:textSize="16dp"/>
            <TextView
                android:layout_weight="5"
                android:layout_width="130dp"
                android:layout_marginRight="5dp"
                android:text="Pre Assessment ID"
                android:fontFamily="@font/worksans_medium"
                android:textStyle="bold"
                android:textColor="@color/button"
                android:textSize="16dp"/>
            <TextView
                android:layout_weight="5"
                android:layout_width="90dp"
                android:text="Password"
                android:fontFamily="@font/worksans_medium"
                android:textStyle="bold"
                android:textColor="@color/button"
                android:textSize="16dp"/>
        </TableRow>

    </TableLayout>
</RelativeLayout>
    </ScrollView>
    </HorizontalScrollView>
        </android.support.v7.widget.CardView>
    </RelativeLayout>
</RelativeLayout>
</ScrollView>
</RelativeLayout>